public class ReferencePrimitiveTypes{
	
    public static void main(String[] args) {
        Integer myInt = null;
        myInt = 5;
        System.out.print("The value of myInt: ");
        System.out.println(myInt);

        myInt = null;
        System.out.print("The value of myInt: ");
        System.out.println(myInt);

    }
}