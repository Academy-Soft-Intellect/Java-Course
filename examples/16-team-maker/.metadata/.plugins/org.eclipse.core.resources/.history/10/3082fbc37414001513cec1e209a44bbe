import java.util.ArrayList;

public class Settings
{
	private ArrayList<String> playersList;

    private int numberOfTeams;

    public ArrayList<String> get_playersList() {
		return playersList;
	}

	public void set_playersList(ArrayList<String> _playersList) {
		this.playersList = _playersList;
	}

	public int get_numberOfTeams() {
		return numberOfTeams;
	}

	public void set_numberOfTeams(int _numberOfTeams) {
		this.numberOfTeams = _numberOfTeams;
	}
}