
public class Properties {

	public static void main(String[] args) {
		
		Cat josefine = new Cat("Josefine", "Persian", 2);
		
		josefine.sayMiau(true);
		
		josefine.sayMiau(false);	
		
		System.out.println()
		;
		Cat harriet = new Cat("Harriet", "Siamese", 5);
		
		harriet.sayMiau(true);
		
		
		harriet.setAge(-2);
		
		
		harriet.sayMiau(true);
		
	}

}
