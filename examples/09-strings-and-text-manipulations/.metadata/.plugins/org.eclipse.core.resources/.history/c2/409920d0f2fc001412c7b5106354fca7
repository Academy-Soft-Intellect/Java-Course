
public class StringConcatenationi {
	public static void main(String[] args) {
		
		StringBuilder sb = new StringBuilder();
        
		sb.append("Така ");
		sb.append("се ");
		sb.append("конкатенират ");
		sb.append("стрингове ");
		sb.append("ефективно ");
		sb.append(" :) ");
		      
		System.out.println(sb.toString());
		
		

	}
}
