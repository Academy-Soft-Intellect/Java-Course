import java.util.LinkedList;
import java.util.Queue;;

public class QueueUsage {

	public static void main(String[] args) {
		
		Queue<Integer> queue = new LinkedList<Integer>();
		
		queue.offer("One");
		queue.offer("Two");
		queue.offer("Three");
		queue.offer("Four");

		
		while (!queue.isEmpty()) {
		    System.out.println(queue.poll());
		}
	}

}
