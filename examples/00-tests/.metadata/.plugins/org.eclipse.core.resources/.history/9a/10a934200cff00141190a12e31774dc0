public class Teacher extends Person implements CheckHomework {

	private String subjectName;
	
	public Teacher() {
		super();
	}
	
	public Teacher(String firstName, String lastName, int yearsOfWork) {
		super(firstName, lastName, yearsOfWork);
	}
	
	public Teacher(String firstName, String lastName, int yearsOfWork, String subjectName) {
		super(firstName, lastName, yearsOfWork);
		this.subjectName = subjectName;
	}

	@Override
	public void checkHomework() {
		System.out.println("Teacher: checking homework.");
	}
	
	public void goodbyeGreet() {
		System.out.println("Teacher: Goodbye!");
	}
}
