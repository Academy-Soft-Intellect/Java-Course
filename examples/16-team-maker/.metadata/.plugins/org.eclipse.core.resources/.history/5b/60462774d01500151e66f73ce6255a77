import java.util.Date;


public class Item {
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getValue() {
		return value;
	}
	public void setValue(double value) {
		this.value = value;
	}
	public Item(Dae date, String name, double value) {
		super();
		this.date = date;
		this.name = name;
		this.value = value;
	}
	private Date date;
	private String name;
	private double value;

}
