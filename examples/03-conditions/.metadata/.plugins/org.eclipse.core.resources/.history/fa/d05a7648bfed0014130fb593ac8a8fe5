public class AndOrFullOperation {
	
	 public static void main(String[] args) {
		 int d = 5;
		 
		 if (d== 7 && d++ ==6){
			 System.out.println("Without entering here!");
		 }
		 System.out.println(d);
		 
		 if (d== 7 & d++ ==6){
			 System.out.println("Without entering here!");
		 }
		 System.out.println(d);
	 }
	

}
