public class Variables {
	  public static void main(String[] args) {
		  
		  int num = 51;
		  System.out.println(num);
		  
		  int newNum = num;
		  System.out.println(newNum);
		  
		  num = 102;
		  System.out.println(num);
		  System.out.println(newNum);
		  
		  int d;
		  System.out.println(d);
		  
	  }
}
