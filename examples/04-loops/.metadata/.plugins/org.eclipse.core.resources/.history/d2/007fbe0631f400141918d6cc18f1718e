
public class NestedLoops {

	public static void main(String[] args) {
		
		int rows = 9;
		int cols = 6;
		
		for (int i = 1; i <= rows; i++) {
			
			for (int j = 1; j <= cols; j++) {
				System.out.printf("%d X %d", i, j);
				if (j != cols) {
					System.out.print(" | ");
				}
			}
			
			System.out.println();
		}
	}

}
