
public class FormattingCode {
	public static void main(String[] arg)
	{
	  int value=010, i=5, w;
	  switch(value){case 10:w=5;System.out.println(w);break;case 9:i=0;break;
	          case 8:System.out.println("8 ");break;
	     default:System.out.println("def ");{
	    	 System.out.println("Ninja ");	}
	     for (int k = 0; k < i; k++, System.out.println(k - 'f'));break;} { System.out.println("loop!"); }
	}


}
