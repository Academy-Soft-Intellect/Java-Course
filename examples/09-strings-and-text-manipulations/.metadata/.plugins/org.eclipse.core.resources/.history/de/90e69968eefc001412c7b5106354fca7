
public class MethodToString {

	public static void main(String[] args) {
		Person gerogi = new Person("Георги", "Билюков");
		
		String personName = gerogi.toString();
		
		System.out.println("personName: " + personName);
		
		String greeting = "Аз съм " + gerogi;
		
		System.out.println(greeting);
		
		Person maria = new Person("Мария", "Петкова");
		
		System.out.format("Нейното име е %s", maria);
		
		

	}

}
