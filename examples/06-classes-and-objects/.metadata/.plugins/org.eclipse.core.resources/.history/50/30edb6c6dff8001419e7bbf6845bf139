
public class Animal extends Creature {

	private float height;
	
	public Animal(int age, float height){
		super.age = age;
		this.height = height;
	}
	

	public float getHeight() {
		return height;
	}

	public void setHeight(float height) {
		this.height = height;
	}

	public void greet() {
		System.out.printf("I am na animal and I am %d years old.\n My Height is %f.\n ", super.age, this.height);
	}

}
