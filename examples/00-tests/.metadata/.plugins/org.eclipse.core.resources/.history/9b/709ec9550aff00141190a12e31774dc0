package homework6;

public class Professor extends Lecturer {
	
	private String title;
	
	public Professor() {
		super();
	}
	
	public Professor(String name, String lastName, int yearsOfWork, String title) {
		super(name, lastName, yearsOfWork);
		this.title = title;
	}
	
	public void info() {
		System.out.println("Title: " + title);
		System.out.println("Years of working experience: " + this.getYearsOfWork());
	}
	
	public void checkHomework() {
		System.out.println("Professor: checking homework.");
	}
	
	public void exam() {
		System.out.println("Professor: exam");
	}
}
