package comparison;

public class StringEquals {

	public static void main(String[] args) {
		
		String first = "I am writing in Java!";
		
		String second = "I am" + " writing in " + "Java!";
		
		String third = second;
		
		boolean refComparisonFirstSecond = first == second;
		
		System.out.format("Reference Comparison With ==: %s \n", refComparisonFirstSecond);
		
		boolean refComparisonSecondThird = second == third;
		
		boolean valueComparison = first.equals(second);
		
		System.out.format("Ð¡Ñ€Ð°Ð²Ð½Ñ?Ð²Ð°Ð½Ðµ Ð½Ð° Ñ?Ñ‚Ñ€Ð¸Ð½Ð³Ð¾Ð²Ðµ Ñ? equals: %s", valueComparison);

	}
}
