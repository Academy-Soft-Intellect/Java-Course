import Interfaces.Hourse;


public class ExceptionBasic {
	
	public static void main(String[] args) {
		try {
			int a = 5 / 0;
		} catch (Exception exception) {
			System.out.println("Прихванато изключение: " + exception.getMessage());
		} finally {
			System.out.println("Този блок се изпълнява винаги!");
		}
	}
	
}	
