public class ReferencePrimitiveTypes{
	
    public static void main(String[] args) {
        Integer myInt = null;
        myInt = 5;
        System.out.print("The value of myInt: ");
        System.out.println(myInt);
        System.out.print("The value of myInt + 7: ");
        System.out.println(myInt + 7);

        myInt = null;
        System.out.print("The value of myInt: ");
        System.out.println(myInt);
        
        

    }
}