public class SpeedTestConcatenation {

	public static void main(String[] args) {
		String concatenator = "Repeated Sentances ";
		String str1 = "";
		
		Date start = new Date();
		for(int i = 0; i < 50000; i++){
			str1 += concatenator;
		}
		Date end = new Date();
		

	}

}
