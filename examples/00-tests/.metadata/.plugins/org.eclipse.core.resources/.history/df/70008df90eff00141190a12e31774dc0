
public class Professor extends Lecturer {
	
	private String title;
	
	public Professor() {
		super();
	}
	
	public Professor(String name, String lastName, int yearsOfWork, String title) {
		super(name, lastName, yearsOfWork);
		this.title = title;
	}
	
	public void printInformation() {
		System.out.println("Title: " + title);
		System.out.println("Years of working experience: " + this.getExperienceInYears());
	}
	
	public void checkHomework() {
		System.out.println("Checking homework from Professor.");
	}
	
	public void createExam() {
		System.out.println("Create exam!");
	}
}
