
public class Human extends Creature {

	private String name;
	
	public Human(String name, int age){
		setName(name);
		setAge(age);
	}
	
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}

	public void greet() {
		System.out.printf("Hello I am %s.\n I am %d years old\n", this.name, super.age);
	}

}
